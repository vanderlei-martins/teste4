        "body-parser": "^1.19.1",
        "ejs": "^3.1.6",
        "express": "^4.16.2",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.7.1",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.14.0",
        "sqlite3": "^5.0.2"
    },
    "devDependencies": {
        "concurrently": "^3.5.0",
        "nodemon": "^2.0.15",
        "sequelize-cli": "^6.4.1"
    }
}
